{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "xamin-angular": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/xamin-angular",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "src/assets/js/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.js",
              "src/assets/js/appear.js",
              "src/assets/js/countdown.js",
              "src/assets/js/jquery.countTo.js",
              "node_modules/magnific-popup/dist/jquery.magnific-popup.js",
              "node_modules/owl.carousel/dist/owl.carousel.js",
              "src/assets/js/Chart.min.js",
              "src/assets/js/Chart.bundle.min.js",
              "src/assets/js/slick.min.js",
              "node_modules/wowjs/dist/wow.js",
              "src/assets/js/isotope.pkgd.min.js",
              "src/assets/revslider/js/revolution.tools.min.js",
              "src/assets/revslider/js/rs6.min.js",
              "src/assets/js/revslider-custom.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "options": {
            "port": 4200,
            "host": "192.168.0.102",
            "disableHostCheck": true,
            "browserTarget": "client:build"
          },
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "xamin-angular:build:production"
            },
            "development": {
              "browserTarget": "xamin-angular:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "xamin-angular:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "src/assets/js/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.js",
              "src/assets/js/appear.js",
              "src/assets/js/countdown.js",
              "src/assets/js/jquery.countTo.js",
              "node_modules/magnific-popup/dist/jquery.magnific-popup.js",
              "node_modules/owl.carousel/dist/owl.carousel.js",
              "src/assets/js/Chart.min.js",
              "src/assets/js/Chart.bundle.min.js",
              "src/assets/js/slick.min.js",
              "node_modules/wowjs/dist/wow.js",
              "src/assets/js/isotope.pkgd.min.js",
              "src/assets/revslider/js/revolution.tools.min.js",
              "src/assets/revslider/js/rs6.min.js",
              "src/assets/js/revslider-custom.js"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "5ec8af69-d729-4200-978b-484b6017ec69"
  }
}
